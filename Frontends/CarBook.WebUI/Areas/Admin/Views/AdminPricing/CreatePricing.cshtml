@model CreatePricingDto
@{
    ViewData["Title"] = "CreatePricing";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="page-content">
    <div class="container-fluid">


        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">

                        <h4 class="card-title">Yeni Ödeme Periyodu Girişi</h4>
                        <p class="card-subtitle mb-4">Ödeme Periyodu Alanına Veri Girişini Aşağıdaki Panelden Yapabilirsiniz.</p>

                        <form method="post">

                            <div class="form-group">
                                <label for="exampleInputTitle">Ödeme Periyot Başlığı</label>
                                <input type="text" class="form-control" id="exampleInputTitle" asp-for="Name" placeholder="Ödeme Periyot Adını Giriniz(Günlük/Haftalık/Aylık)">
                            </div>
                           



                            <button type="submit" class="btn btn-primary waves-effect waves-light">Ödeme Türünü Kaydet </button>
                        </form>

                    </div>
                </div>


            </div>

        </div>


    </div>
</div>

