@model CreateServiceDto
@{
    ViewData["Title"] = "CreateService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}





<div class="page-content">
    <div class="container-fluid">


        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">

                        <h4 class="card-title">Hizmet Sayfası Veri Girişi Alanı.</h4>
                        <p class="card-subtitle mb-4">  Ana Sayfa Ve Hizmetler Bölümünde Yer Alan Hizmet Listesinde Yer Alan Hizmet Listesindeki İşlemleri Gerçekleştirebilirsiniz.</p>

                        <form method="post">

                            <div class="form-group">
                                <label for="exampleInputTitle">Hizmet Başklığı</label>
                                <input type="text" class="form-control" id="exampleInputTitle" asp-for="Title" placeholder="Hizmet Başlığını Giriniz">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputDescription">Hizmet Açıklaması</label>
                                <input type="text" class="form-control" id="exampleInputDescription" asp-for="Description" placeholder="Hizmet Açıklaması Giriniz">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputIconUrl">Hizmet Ikon Url</label>
                                <input type="text" class="form-control" id="exampleInputIconUrl" asp-for="IconUrl" placeholder="Hizmet Ikon Url Giriniz">
                            </div>



                            <button type="submit" class="btn btn-success waves-effect waves-light">Hizmet Bilgisini Kaydet </button>
                        </form>

                    </div>
                </div>


            </div>

        </div>


    </div>
</div>





