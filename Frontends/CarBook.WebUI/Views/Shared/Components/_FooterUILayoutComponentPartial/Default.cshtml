@model List<ResultFooterAddressDto>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>

<script>
    $(document).ready(function () {
        $('#btn1').click(function () {
            swal("Şartlar & Koşullar"," Araba kiralama sürecine dair tüm hizmet koşullarına rezervasyonun fiziki olarak gerçekleştireleceiği ofislerimizde yer aln anlaşma dokümanlarından ulaşabilirsiniz.Bütün sürecin hasar,ceza,geçişler(Hgs,Eds,Ogs,vs) ile ilgili detaylar yer almaktadır keyifli yolculuklar dileriz.");
        });
        $('#btn2').click(function () {
            swal("Güvenlik", "CarBook projemiz AspNet Core teknolojisnde Sha256 şifreleme tekniğiyle tamamen şifreli olarak tutulması alt yapısına sahiptir.Sistemizmde Jwt doğrulamsı yapılmaktadır. Kullaıcılarımız ödeme yaptıkları kart bilgileri sistemde kesinlikle saklanmamamaktadır. Tc Kimlik Numarası,Mail Adresi ve telefon gibi bilgileri ise şifrelenerek tutulmaktadır. ");
        });
        $('#btn3').click(function () {
            swal("Çerez Politakası", "Sizlere daha iyi bir hizmet verebilmek için çerez politaksı kullanmaktayız. ");
        });
        $('#btn4').click(function () {
            swal("Sıkça Sorulan Sorular", "Sistem ile ilgili sık sorulan sorular yer alır. ");
        });
        $('#btn5').click(function () {
            swal("Ödeme Yöntemleri", "Ödeme işlemeleri Yanlızca Fiziki Lokasyonlarımda Kart İle Yapılmaktadır.. ");
        });
        $('#btn6').click(function () {
            swal("Nasıl Çalışır", "1.Adım:Aracını Kiralayın \n 2.Adım:Mailiniz Kontrol Edin \n 3.Adım:Lokasyonumza Gelin ");
        });
   
    });
</script>
@foreach (var item in Model)
{
    <footer class="ftco-footer ftco-bg-dark ftco-section">
    <div class="container">
        <div class="row mb-5">
            <div class="col-md">
                <div class="ftco-footer-widget mb-4">
                    <h2 class="ftco-heading-2"><a href="#" class="logo">Car<span>book</span></a></h2>
                    <p>@item.Description</p>
                    <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                        <li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
                        <li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
                        <li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
                    </ul>
                </div>
            </div>
            <div class="col-md">
                <div class="ftco-footer-widget mb-4 ml-md-5">
                    <h2 class="ftco-heading-2">Hızlı Linkler</h2>
                    <ul class="list-unstyled">
                        <li><a href="/About/Index" class="py-2 d-block">Hakkımızda</a></li>
                        <li><a href="/Car/Index" class="py-2 d-block">Hizmetlerimiz</a></li>
                        <li><a  href="#" id="btn1" class="py-2 d-block">Şartlar & Koşullar</a></li>
                        <li><a href="#" id="btn2" class="py-2 d-block">Güvenlik</a></li>
                        <li><a href="#" id="btn3" class="py-2 d-block">Çerez Politikası</a></li>
                    </ul>
                </div>
            </div>
            <div class="col-md">
                <div class="ftco-footer-widget mb-4">
                    <h2 class="ftco-heading-2">Müşteri Desteği</h2>
                    <ul class="list-unstyled">
                        <li><a href="#" id="btn4" class="py-2 d-block">Sıkça Sorulanlar</a></li>
                            <li><a href="#" id="btn5" class="py-2 d-block">Ödeme Yöntemleri</a></li>
                        <li><a href="/CarPricng/Index" class="py-2 d-block">Kiralama</a></li>
                            <li><a href="#" id="btn6" class="py-2 d-block">Nasıl Çalışır</a></li>
                        <li><a href="/Contact/Index" class="py-2 d-block">Bize Yazın</a></li>
                    </ul>
                </div>
            </div>
            <div class="col-md">
                <div class="ftco-footer-widget mb-4">
                    <h2 class="ftco-heading-2">Bir sorunuz mu var?</h2>
                    <div class="block-23 mb-3">
                        <ul>
                            <li><span class="icon icon-map-marker"></span><span class="text">@item.Address</span></li>
                            <li><a href="#"><span class="icon icon-phone"></span><span class="text">@item.Phone</span></a></li>
                            <li><a href="#"><span class="icon icon-envelope"></span><span class="text">@item.Email</span></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 text-center">

                <p>
                   
                    Copyright &copy;<script>document.write(new Date().getFullYear());</script> Tüm Hakları Saklıdır.| Yasin Yaman <i class="icon-heart color-danger" aria-hidden="true"></i>
                  
                </p>
            </div>
        </div>
    </div>
</footer>
}

<div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>

