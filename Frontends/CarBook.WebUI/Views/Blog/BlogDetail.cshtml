
@{
    ViewData["Title"] = "BlogDetail";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<section class="ftco-section ftco-degree-bg">
    <div class="container">
        <div class="row">
            @await Component.InvokeAsync("_BlogDetailsMainComponentPartial",new{id=ViewBag.blogid})
            <div class="col-md-4 sidebar ftco-animate">
                @await Component.InvokeAsync("_BlogDetailSideBarSearchBocComponentPartial")
               

                @await Component.InvokeAsync("_BlogDetailsCategoryComponentsPartial")

                @await Component.InvokeAsync("_BlogDetailsRecentBlogsComponentPartial")

                @await Component.InvokeAsync("_BlogDetailTagCloudComponentPartial")

                @await Component.InvokeAsync("_BlogDetailParagraphComponentPartial")
                
                
            </div>

        </div>
    </div>
</section>

